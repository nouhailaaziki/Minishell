L33tShell-N.Y$ touch 'hello"4'
L33tShell-N.Y$ echo $a
h"*"
L33tShell-N.Y$ echo *
hello4 hello1 hello2 hello3 ls

bash-3.2$ echo *
hello"4 hello1 hello2 hello3 ls




